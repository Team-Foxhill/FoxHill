  Sheet1:
  - QuestNumber: 1
    QuestShowTime: 120
    PreCondition: MinPlayerLevel(1);
    StartNPC: 10001
    Objective: Kill(1, 10);
    TimeLimit: 180
    EndNPC: 10001
    StartDialogue: "\uC694\uC998 \uB4E4\uC5B4 \uC5B4\uB451\uC2DC\uB2C8\uB4E4\uC774
      \uC794\uB729 \uB098\uD0C0\uB098\uACE0 \uC788\uB124.\n\uCC38 \uC774\uC0C1\uD55C
      \uC77C\uC774\uC9C0.\n\uC774 \uD604\uC0C1\uC5D0 \uB300\uD574 \uC5F0\uAD6C\uB97C
      \uD574\uBCF4\uACE0 \uC2F6\uC740\uB370,\n\uD639\uC2DC \uC5F4 \uB9C8\uB9AC\uB9CC
      \uC7A1\uC544\uB2E4 \uC904 \uC218 \uC788\uB294\uAC00?"
    ProgressDialogue: "\uC5B4\uB451\uC2DC\uB2C8 \uC5F4 \uB9C8\uB9AC\uBA74 \uB3FC.\n\uB124\uAC90
      \uC5B4\uB824\uC6B4 \uC77C\uC774 \uC544\uB2C8\uC796\uB098?\n\uBD80\uD0C1 \uC880
      \uD558\uACA0\uB124."
    SuccessDialogue: "\uC624\uD638, \uBAB8\uC774 \uCEE4\uC9C0\uC9C0 \uC54A\uACE0
      \uAC1C\uCCB4 \uC218\uB9CC \uB298\uC5B4\uB09C\uB2E4\uB77C...\n\uC2E0\uBE44\uD55C
      \uD604\uC0C1\uC774\uAD70.\n\uC544\uBB34\uB798\uB3C4 \uC5B4\uB5A4 \uC2E0\uB839\uD55C
      \uD798\uC774 \uC5B4\uB460\uC744\n\uBB49\uCE58\uCE58 \uBABB\uD558\uAC8C \uB9CC\uB4E4\uACE0
      \uC788\uB294 \uAC83 \uAC19\uB124.\n\uADF8 \uB355\uC5D0 \uC9C0\uAE08\uC740 \uBC84\uD2F8
      \uC218 \uC788\uB294 \uAC8C\uC57C.\n\uBD80\uB514 \uADF8 \uD798\uC774 \uB5A8\uC5B4\uC9C0\uAE30
      \uC804\uC5D0 \uBB3C\uB7EC\uB098\uBA74 \uC88B\uACA0\uAD6C\uB824."
    FailDialogue: "\uC790\uB124 \uC2E4\uB825\uC774 \uADF8 \uC815\uB3C4\uB3C4\n\uC548
      \uB420 \uAC70\uB77C\uACE4 \uC0DD\uAC01\uB3C4 \uBABB \uD588\uB124.\n\uD558\uC9C0\uB9CC
      \uC5B4\uCA4C\uACA0\uB294\uAC00?\n\uC774\uC81C\uBD80\uD130\uB77C\uB3C4 \uC218\uD589\uC744
      \uC5F4\uC2EC\uD788 \uD558\uB294 \uC218\uBC16\uC5D0.\n\uC790, \uB0B4 \uD798\uC744
      \uC880 \uB098\uB220 \uC904 \uD130\uC774\uB2C8 \uC218\uD589\uC5D0 \uC815\uC9C4\uD558\uC2DC\uAC8C."
    Reward: Exp(100);
    Penalty: Exp(120);
  - QuestNumber: 2
    QuestShowTime: 300
    PreCondition: 'MinPlayerLevel(1);

      ClearQuest(1);'
    StartNPC: 10001
    Objective: 'Deliver(5);

      Talk();'
    TimeLimit: 60
    EndNPC: 10002
    StartDialogue: "\uC774\uBC88\uC5D0\uB3C4 \uC790\uB124\uC5D0\uAC8C \uBD80\uD0C1\uD574\uC11C
      \uBBF8\uC548\uD558\uB124\uB9CC,\n\uB0B4 \uB3D9\uC0DD\uC5D0\uAC8C \uBD80\uC801\uC744
      \uC804\uD574\uC8FC\uACA0\uB098.\n\uC694\uC998 \uC694\uAD34\uB4E4\uC774 \uC790\uAFB8
      \uBC00\uB824\uB4DC\uB294 \uAC8C \uBD88\uC548\uD574\uC11C \uB9D0\uC77C\uC138.\n\uD5CC\uB370
      \uC774 \uBD80\uC801\uC740 \uC624\uB798\uB418\uBA74 \uC0AC\uC6A9\uD560 \uC218
      \uC5C6\uB2E4\uB124.\n\uADF8\uB798\uC11C \uC790\uB124\uC758 \uB3C4\uC6C0\uC774
      \uD544\uC694\uD558\uB124.\n\uB3C4\uC0AC\uB4E4\uC740 \uCD95\uC9C0\uBC95\uC744
      \uC4F8 \uC218 \uC788\uC9C0 \uC54A\uC740\uAC00.\n\uD639\uC2DC\uB098 \uBB34\uC2A8
      \uB9D0\uC778\uC9C0 \uBAA8\uB974\uACA0\uB2E4\uBA74,\n\uBA38\uB9BF\uC18D\uC5D0
      \uAC08 \uC9C0(z)\uC790\uB97C \uB5A0\uC62C\uB9AC\uAC8C.\n\uADF8 \uC18C\uB9DD\uC774
      \uC778\uAC04\uACC4\uB97C \uB118\uC5B4 \uC625\uD669\uC0C1\uC81C\uB2D8\uAED8
      \uB2FF\uB294\uB2E4\uBA74\n\uBC14\uB78C\uCC98\uB7FC \uB2EC\uB824\uB098\uAC00\uB294
      \uD798\uC744 \uADF8\uBD84\uAED8\uC11C \uC904 \uAC78\uC138.\n\uADF8\uB7EC\uB2C8
      \uC774 \uBD80\uC801\uC744 \uC18D\uD788 \uC804\uD574\uC8FC\uAC8C.\n\uB3D9\uC0DD\uC740
      \uC2E0\uBAA9 \uC544\uB798 \uC0B0\uB2E4\uB124.\n\uADF8\uB7FC \uC798 \uBD80\uD0C1\uD558\uACA0\uB124."
    ProgressDialogue: "\uB0B4 \uB3D9\uC0DD\uC740 \uC2E0\uBAA9 \uC544\uB798\uC5D0
      \uC788\uB124.\n\uAFB8\uBB3C\uAC70\uB9AC\uC9C0 \uB9D0\uAC8C.\n\uC2DC\uAC04\uC774
      \uB0A8\uC544 \uB3C4\uB294\uAC00?\n\uB9C8\uC74C \uC18D\uC5D0 \uAC08 \uC9C0\uB97C
      \uADF8\uB9AC\uAC8C.\n\uC625\uD669\uC0C1\uC81C\uB2D8, \uBE44\uB098\uC774\uB2E4..."
    SuccessDialogue: "\uC6B0\uB9AC \uD615\uB2D8\uC774 \uBCF4\uB0B4\uC168\uB2E4\uACE0?\n\uD5C8\uD5C8,
      \uAC71\uC815\uB3C4 \uB9CE\uC73C\uC2DC\uAD70.\n\uB098\uB294 \uC544\uC9C1 \uAD1C\uCC2E\uB2E4\uB9CC...\n\uC2E0\uC138\uB97C
      \uC84C\uC73C\uBA74 \uBE48 \uC190\uC73C\uB85C \uB3CC\uB824\uBCF4\uB0BC \uC21C
      \uC5C6\uB294 \uBC95.\n\uC774 \uB2E8\uC57D\uC744 \uB4DC\uC2DC\uAC8C.\n\uBD84\uBA85
      \uC790\uB124\uC5D0\uAC8C \uB3C4\uC6C0\uC774 \uB420 \uAC78\uC138."
    FailDialogue: "\uD5C8\uD5C8, \uADF8 \uBD80\uC801\uC740 \uC624\uB798\uB418\uBA74
      \uC0AC\uC6A9\uD560 \uC218 \uC5C6\uB2E4\uACE0 \uD588\uAC70\uB298.\n\uC790\uB124\uAC00
      \uB0B4\uAC8C \uC774\uB807\uAC8C \uC2E4\uB9DD\uC744 \uC548\uAE38 \uC904\uC774\uC57C."
    Reward: Item(1, 1);
    Penalty: Spawn(1, 100);
